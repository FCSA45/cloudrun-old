<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="nacos-console" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} %-5level %logger{36} - %msg%n</pattern>
        </encoder>
    </appender>
    <logger name="com.alibaba.nacos" level="WARN" additivity="false">
        <appender-ref ref="nacos-console"/>
    </logger>
</configuration>